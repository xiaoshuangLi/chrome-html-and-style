{
  "manifest_version": 2,
  "name": "HTML and Style",
  "version": "0.0.1",
  "author": "Xiaoshuang Li <qq1157502759@gmail.com>",
  "minimum_chrome_version": "66.0.0",
  "description": "Get html and style for element.",
  "devtools_page": "devtools.html",
  "web_accessible_resources": [
    "content.js"
  ],
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./inject.js"]
    }
  ]
}
